<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <TextView
        android:id="@+id/user_myinfo_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/myinfo_info_title_mg_t"
        android:layout_marginLeft="@dimen/myinfo_info_title_mg_l"
        android:gravity="center_horizontal"
        android:text="@string/myinfo_info_myinfo_name"
        android:textSize="@dimen/vip_package_vip_title_size"
        android:textColor="@color/color_ee"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/myinfo_info_content_mg_t"
        android:layout_marginLeft="@dimen/myinfo_info_content_mg_lr"
        android:layout_marginRight="@dimen/myinfo_info_content_mg_lr"
        android:layout_marginBottom="@dimen/myinfo_info_content_mg_b"
        android:orientation="horizontal"
        android:background="#04ffffff">
        <RelativeLayout
            android:layout_width="@dimen/myinfo_info_lift_v_w"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/user_head_info_image"
                android:layout_marginTop="@dimen/myinfo_info_user_head_mg_t"
                android:layout_marginLeft="@dimen/myinfo_info_user_head_mg_l"
                android:layout_width="@dimen/myinfo_info_user_head_w"
                android:layout_height="@dimen/myinfo_info_user_head_w"/>

            <TextView
                android:id="@+id/my_info_head_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/user_head_info_image"
                android:layout_marginTop="@dimen/myinfo_info_head_text_mg_t"
                android:layout_marginLeft="@dimen/myinfo_info_head_text_mg_l"
                android:layout_centerHorizontal="true"
                android:text=""
                android:textSize="@dimen/myinfo_info_detail_ts"
                android:textColor="@color/info_info_text_a8"/>

        </RelativeLayout>

        <View android:layout_width="@dimen/myinfo_info_line_w"
              android:layout_height="match_parent"
              android:layout_marginTop="@dimen/myinfo_info_line_mg_t"
              android:layout_marginBottom="@dimen/myinfo_info_line_mg_t"
              android:background="@drawable/user_info_icon_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/myinfo_info_item_margin_t"
            android:layout_marginLeft="@dimen/myinfo_info_item_margin_l"
            android:layout_marginRight="@dimen/myinfo_info_item_margin_r"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/myinfo_info_item_h">
                <ImageView
                    android:id="@+id/my_info_detail_vip_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/myinfo_info_item_image_margin_l"
                    android:src="@drawable/user_info_head_normol"/>
                <TextView
                    android:id="@+id/my_info_detail_10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/myinfo_info_item_text_margin_l"
                    android:gravity="left"
                    android:text="@string/user_info_ipname"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
                <TextView
                    android:id="@+id/my_info_detail_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/myinfo_info_item_text_margin_r"
                    android:text="@string/user_info_ipname_normol"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
            </RelativeLayout>

            <View android:layout_width="match_parent"
                  android:layout_height="@dimen/myinfo_info_line_s_h"
                  android:background="#141c20"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/myinfo_info_item_h">
                <ImageView
                    android:id="@+id/my_info_detail_money_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/myinfo_info_item_image_margin_l"
                    android:src="@drawable/user_info_head_money"/>
                <TextView
                    android:id="@+id/my_info_detail_20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/myinfo_info_item_text_margin_l"
                    android:text="@string/myinfo_acount_money_name"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
                <TextView
                    android:id="@+id/my_info_detail_21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/myinfo_info_item_text_margin_r"
                    android:text="@string/user_zero_money"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
            </RelativeLayout>

            <View android:layout_width="match_parent"
                  android:layout_height="@dimen/myinfo_info_line_s_h"
                  android:background="#141c20"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/myinfo_info_item_h">
                <ImageView
                    android:id="@+id/my_info_detail_credit_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/myinfo_info_credit_image_mg_l"
                    android:paddingTop="@dimen/myinfo_info_credit_image_pd_t"
                    android:src="@drawable/user_info_icon_credit_red"/>
                <TextView
                    android:id="@+id/my_info_detail_30"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/myinfo_info_item_text_margin_l"
                    android:text="@string/myinfo_credit_canused_money"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
                <TextView
                    android:id="@+id/my_info_detail_31"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/myinfo_info_item_text_margin_r"
                    android:text="@string/user_zero_money"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
            </RelativeLayout>

            <View android:layout_width="match_parent"
                  android:layout_height="@dimen/myinfo_info_line_s_h"
                  android:background="#141c20"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/myinfo_info_item_h">
                <ImageView
                    android:id="@+id/my_info_detail_month_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/myinfo_info_month_image_mg_l"
                    android:layout_marginTop="@dimen/myinfo_info_month_image_pd_t"
                    android:src="@drawable/user_info_icon_month"/>

                <TextView
                    android:id="@+id/my_info_detail_40"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/myinfo_info_item_text_margin_l"
                    android:text="@string/myinfo_credit_monthvip_name"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
                <TextView
                    android:id="@+id/my_info_detail_41"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/myinfo_info_item_text_margin_r"
                    android:text="@string/myinfo_credit_monthvip_off"
                    android:textSize="@dimen/myinfo_info_detail_ts"
                    android:textColor="@color/info_info_text_a8"/>
            </RelativeLayout>

        </LinearLayout>


    </LinearLayout>

</RelativeLayout>